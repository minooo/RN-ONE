{
  "presets": ["react-native"],
  "plugins": [
    "transform-decorators-legacy",
    [
      "import",
      {
        "libraryName": "antd-mobile"
      }
    ],
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": ["./src"],
        "alias": {
          "@components": "./src/components",
          "@utils": "./src/utils",
          "@style": "./src/static/style"
        },
        "extensions": [".js", ".ios.js", ".android.js"]
      }
    ]
  ]
}
